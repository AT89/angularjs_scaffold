var theApp = angular.module("Client", ['ngCookies']).
  config(function($routeProvider, $locationProvider) {
  $locationProvider.html5Mode(true);
  $routeProvider.when("/", {
    controller: "WelcomeCtrl",
    templateUrl: "<%= asset_path('welcome/index.html') %>"
  }).otherwise({
    redirectTo: "/"
  });
});

